<%= form_with model: [@user, @coupon] do |f| %>
    <%= f.label  :Title %>
    <%= f.text_field  :title %>
    <%= f.label  :Description %>
    <%= f.text_field :description %>
    <%= f.label :Code %>
    <%= f.text_field :code %>
    <%= f.label  :mage %>
    <%= f.file_field  :oupon_file %>
    <%= f.label :Expiry_date %>
    <%= f.text_date :expiry_date %>
    <%= f.label :Coins_needed %>
    <%= f.text_field :coins_needed %>
    <%= f.submit %>
<% end %>
